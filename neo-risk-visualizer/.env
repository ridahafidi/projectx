# Environment Variables for Impactor-2025
# Copy this file to .env and fill in your values

# NASA API Configuration
NASA_API_KEY=DEMO_KEY

# Mapbox Configuration (for 3D Earth visualization)
VITE_MAPBOX_TOKEN=DEMO_TOKEN

# Application Configuration
ENVIRONMENT=development

# Database (optional)
DATABASE_URL=postgresql://impactor:password@localhost:5432/impactor_db

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Security (change in production)
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here

# Monitoring (optional)
SENTRY_DSN=

# Docker Configuration
COMPOSE_PROJECT_NAME=impactor-2025